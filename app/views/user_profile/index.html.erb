                        <%= link_to edit_driving_license_path(@driving_license), remote: true do %><i class="fa fa-pencil" aria-hidden="true"></i> <%end%>
                      <%end%>
                    </div>
                    <div class="profileboxn_con">
                      <div class="hobbies">
                        <div id='drivingLicenseShowDiv'>
                          <% if @driving_license %>
                            <div class='show_icon' id='driving_license_<%=@driving_license.id%>'>
                              <ul>
                                <% if @driving_license.dl_a %>
                                  <li><span><i class="fa fa-motorcycle" aria-hidden="true"></i></span><label>A</label></li>
                                <%end%>
                                <% if @driving_license.dl_a2 %>
                                  <li><span><i class="fa fa-motorcycle" aria-hidden="true"></i></span><label>A2</label></li>
                                <%end%>
