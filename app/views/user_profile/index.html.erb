                                  <li><span><i class="fa fa-motorcycle" aria-hidden="true"></i></span><label>AM</label></li>
                                <%end%>
                                <% if @driving_license.dl_b %>
                                  <li><span><i class="fa fa-car" aria-hidden="true"></i></span><label>B</label></li>
                                <%end%>
                                <% if @driving_license.dl_be %>
                                  <li><span><i class="fa fa-car" aria-hidden="true"></i></span><label>BE</label></li>
