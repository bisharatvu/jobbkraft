                      </div>
                    </div>
                  </div>
                  <!-==================================== Competence Show END ====================================>

                  <!-==================================== Language Show START ====================================>
                  <h id='addMoreLanguages'></h>
                  <div class="profile_boxn">
                    <div class="profileboxn_heading"><%=t('page_profile.language')%><a data-target="#addlanguage" data-toggle="modal"><i class="fa fa-plus-circle" aria-hidden="true"></i></a></div>
                    <div class="profileboxn_con">
                      <div id='languagesShowDiv'>
                        <% if @user_languages.count > 0 %>
                          <% @user_languages.each do |language| %>
                            <div class='show_icon' id='language_<%=language.id%>'>
                              <div class="profileboxn_subheading">
                                <div class="edit_delete_icon">
                                  <%= link_to edit_language_path(language), remote: true do %><i class="fa fa-pencil" aria-hidden="true"></i> <%end%>
                                  <%= link_to language, method: :delete, data: {title: t('page_profile.delete_title'), confirm: "#{t('page_profile.delete_confirm')} '#{language.language_name}'", commit: 'Sure!'} , remote: true do %><i class='fa fa-times-circle' aria-hidden="true"></i> <%end%>
                                </div>
                                <%= language.language_name %>
                              </div>
                              <p><%= language.language_skill %></p>
                            </div>
                          <%end%>
                        <%end%>
                      </div>
                    </div>
                  </div>
                </div>
                <!-==================================== Language Show END ====================================>

                <!-==================================== Certificate Show START ====================================>
                <h id='addMoreCertificates'></h>
                <div class="col-sm-6">
                  <div class="profile_boxn">
                    <div class="profileboxn_heading"><%=t('page_profile.certificate')%><a data-target="#addCertificate" data-toggle="modal"><i class="fa fa-plus-circle" aria-hidden="true"></i></a></div>
                    <div class="profileboxn_con">
                      <div id='certificatesShowDiv'>
                        <% if @user_certificates.count > 0 %>
                          <% @user_certificates.each do |certificate| %>
                            <div class='show_icon' id='certificate_<%=certificate.id%>'>
                              <div class="edit_delete_icon">
                                <%= link_to edit_certificate_path(certificate), remote: true do %><i class="fa fa-pencil" aria-hidden="true"></i> <%end%>
                                <%= link_to certificate, method: :delete, data: {title: t('page_profile.delete_title'), confirm: "#{t('page_profile.delete_confirm')} '#{certificate.certificate_name}'", commit: 'Sure!'} , remote: true do %><i class='fa fa-times-circle' aria-hidden="true"></i> <%end%>
                              </div>
                              <div class="profileboxn_subheading"><%= certificate.certificate_name %>
                                <strong><%= certificate.certificate_publish_date %></strong>
                              </div>
                              <p><%= certificate.certificate_publisher %></p>
                              <p><%= certificate.certificate_description %></p>
                            </div>
                          <%end%>
                        <%end%>
                      </div>
                    </div>
                  </div>
                  <!-==================================== Certificate Show END ====================================>

                  <!-==================================== Driving License Show START ====================================>
