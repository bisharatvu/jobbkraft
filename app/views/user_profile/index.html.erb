<div class="dbbody_wrap">
  <div>
    <div class="dbbody_con">
      <div class="profilesettting_con">
        <div class="profile_imgbtns">
          <div class="row">
<!-==================================== Image Show START ====================================>
            <div class="col-sm-3">
              <div class="profileS_boxesprofile">
                <%if @user.image_url.url%>
                  <h4>
                    <%= image_tag(@user.image_url.url, alt: 'logo', id: 'my_image',  class: 'img-thumbnail img-responsive profileImVi') %>
                  </h4>
                <%else%>
                  <%= image_tag('212x244.png', alt: 'logo', id: 'my_image',  class: 'img-thumbnail img-responsive profileImVi') %>
                <%end%>
                <%= form_for(:image_uploading_form, url: :add_profile_image, remote: true, html: {method: :post, id: 'upload_form', multipart: true}) do |f| %>
                  <div id='imgdiv'>
                    <div class='wrapper'>
                      <%= f.file_field :image_url%>
                    </div>
                    <button class="btn fa fa-camera">  </button>

                  </div>
                <% end %>
              </div>
            </div>
<!-==================================== Image Show START ====================================>

<!-==================================== Video Show START ====================================>
<!-============================ Video Show Display video in LightBox =============================>

            <div class="col-sm-3">
              <div class="profileS_boxesprofile">
                <%if @user.video_url.url%>
                  <h4>
                    <a href= <%= @user.video_url.to_s.html_safe %> class="html5lightbox" data-width="480" data-height="320" title="">
                    <%=  image_tag("video.png", class: 'img-thumbnail img-responsive profileImVi' )%></a>
                  </h4>
                <%else%>
                  <%= image_tag('212x244.png', alt: 'Video', id: 'my_image',  class: 'img-thumbnail img-responsive profileImVi') %>
                <%end%>
                <%= form_for(:video_uploading_form, url: :add_profile_video, remote: true, html: {method: :post, id: 'video_upload_form', multipart: true}) do |f| %>
                  <div id="videodiv">
                    <div class="wrapper">
                      <%= f.file_field :video_url%>
                    </div>
                    <button class="btn"><i class="fa fa-video-camera"></i></button>
                  </div>
                <% end %>
              </div>
            </div>


<!-==================================== Video Show END ====================================>

            <div class="col-sm-6">
              <div class="profile_btns">
                <button class="btn btn-default btn-primary mb10"><i class="fa fa-comment-o"></i><%=t('page_profile.personal_info.contact_me')%></button><br>
                <button class="btn btn-default btn-primary"><%=t('page_profile.personal_info.send_job_agreement')%></button>
              </div>
            </div>
          </div>
        </div>
        <div class="profilesettting_heading">
          <h1>
            <%= @user.user_first_name%> <%= @user.user_last_name%>
          </h1>
          <%= link_to '/linkedin/auth', class: 'btn btn-default btn-primary pull-right' do %><i class="fa fa-linkedin-square" aria-hidden="true"></i><%=t('page_profile.import_linkedin_profile')%><%end%>
        </div>

        <div class="profileS_con">
          <div class="profileS_box">
<!-==================================== About Me Show START ====================================>
            <div class="profile_boxn">
              <div class="profileboxn_heading"><%=t('page_profile.personal_info.profile')%>
                <a data-target="#editAboutMe" data-toggle="modal"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                <!--<a onclick= "editAboutMe();"><i class="fa fa-pencil" aria-hidden="true"></i></a>-->
              </div>
              <div class="profileboxn_con">
                <%if @user.about_me%>
                  <div id='aboutMeShowDiv'>
                  <%= @user.about_me%>
                  </div>
                <%else%>
                  <p id="about_me_text88">
                    <%=t('page_profile.no_about_me')%>
                  </p>
                <%end%>
              </div>
            </div>
<!-==================================== About Me Show END ====================================>
            <div class="row">
<!-==================================== PERSONAL INFO Show START ====================================>
              <div class="col-sm-6">
                <div class="profile_boxn">
                  <div class="profileboxn_heading"><%=t('page_profile.personal_info.personal')%>
                    <a data-target="#editaccountInfo" data-toggle="modal"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                  </div>
                  <div class="profileboxn_con">
                    <div id="showPersonalInfo" class="personal_info">
                      <ul>
                        <li><label><%=t('page_profile.personal_info.first_name')%></label><span><%= @user.user_first_name%></span></li>
                        <li><label><%=t('page_profile.personal_info.last_name')%></label><span><%= @user.user_last_name%></span></li>
                        <li><label><%=t('page_profile.personal_info.mobile_phone')%></label><span><%= @user.user_cell_phone%></span></li>
                        <li><label><%=t('page_profile.personal_info.email_address')%></label><span><%= @user.email%></span></li>
                        <li><label><%=t('page_profile.personal_info.address')%></label><span><%= @user.user_address_street%></span></li>
                        <li><label><%=t('page_profile.personal_info.zip')%></label><span><%= @user.user_address_zip%></span></li>
                        <li><label><%=t('page_profile.personal_info.town')%></label><span><%= @user.user_address_city%></span></li>
                        <li><label><%=t('page_profile.personal_info.personal_number')%></label><span><%= @user.user_personal_number%></span></li>
                        <li><label><%=t('page_profile.personal_info.gender')%></label><span><%= @user.user_gender%></span></li>
                      </ul>
                    </div>
                  </div>
                </div>
<!-==================================== PERSONAL INFO Show END ====================================>

<!-==================================== Experience Show START ====================================>

                <div class="profile_boxn">
                  <div class="profileboxn_heading"><%=t('page_profile.experience')%><a data-target="#addExperiences" data-toggle="modal"><i class="fa fa-plus-circle" aria-hidden="true"></i></a></div>
                  <div class="profileboxn_con">
                    <div id='experiencesShowDiv'>
                      <% if @user_experiences.count > 0 %>
                        <% @user_experiences.each do |experience| %>
                          <div class='show_icon' id='experience_<%=experience.id%>'>
                            <div class="edit_delete_icon">
                              <%= link_to edit_experience_path(experience), remote: true do %><i class="fa fa-pencil" aria-hidden="true"></i> <%end%>
                              <%= link_to experience, method: :delete, data: {title: t('page_profile.delete_title'), confirm: "#{t('page_profile.delete_confirm')} '#{experience.experience_title}'", commit: 'Sure!'} , remote: true do %><i class='fa fa-times-circle' aria-hidden="true"></i> <%end%>
                            </div>
                            <div class="profileboxn_subheading"><%= experience.experience_title %>
                              <strong>
                                <%= experience.experience_date_from %>
                                <% if experience.experience_date_to %>
                                  to
                                <%end%>
                                <%= experience.experience_date_to %>
                              </strong>
                            </div>
                            <p><%= experience.experience_company_name %></p>
                            <p><%= experience.experience_description %></p>
                          </div>
                        <%end%>
                      <%end%>
                    </div>
                  </div>
                </div>
              </div>
<!-==================================== Experience Show END ====================================>

<!-==================================== Education Show START ====================================>
              <div class="col-sm-6">
                <div class="profile_boxn">
                  <div class="profileboxn_heading"><%=t('page_profile.education')%><a data-target="#addEducations" data-toggle="modal"><i class="fa fa-plus-circle" aria-hidden="true"></i></a></div>
                  <div class="profileboxn_con">
                    <div id='educationsShowDiv'>
                      <% if @user_educations.count > 0 %>
                        <% @user_educations.each do |education| %>
                          <div class='show_icon' id='education_<%=education.id%>'>
                            <div class="edit_delete_icon">
                              <%= link_to edit_education_path(education), remote: true do %><i class="fa fa-pencil" aria-hidden="true"></i> <%end%>
                              <%= link_to education, method: :delete, data: {title: t('page_profile.delete_title'), confirm: "#{t('page_profile.delete_confirm')} '#{education.education_title}'", commit: 'Sure!'} , remote: true do %><i class='fa fa-times-circle' aria-hidden="true"></i> <%end%>
                            </div>
                            <div class="profileboxn_subheading"><%= education.education_title %>
                              <strong><%= education.education_date_from %> to <%= education.education_date_to %></strong>
                            </div>
                            <p><%= education.education_school_name %></p>
                            <p><%= education.education_description %></p>
                          </div>
                        <%end%>
                      <%end%>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>


   <!-==================================== SKILLS DIV START ====================================>
