<div class="static_header">
  <div class="dbheader_wrap">
    <div class="dbheader_con">
      <button class="navbar-toggle navbar_toggle" id="show_left_menu" type="button">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span> <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <div class="db_logo"><%= image_tag("beta_logo.png", alt: "logo")%></div>
      <div class="profile_menu">
        <div class="notification_box">
          <div class="notification_menu">
            <span><i class="fa fa-bolt" ></i> <em><%= t('navbar.notifications')%></em> </span>
            <ul>
              <li><a href="#">No Notification</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


